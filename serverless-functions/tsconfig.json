{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "functions/src/lib"
  },
  "include": ["functions/**/*.ts"],
  "exclude": ["node_modules", "functions/lib"]
}
